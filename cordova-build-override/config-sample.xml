<?xml version='1.0' encoding='utf-8'?>
<widget id="info.hy-cloud.sharebj" version="0.1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>sharebj</name>
    <description>Shared Baby Journals</description>
    <author email="yanjb@hy-cloud.info" href="http://sharebj.hy-cloud.info">Byrne Yan</author>
    <preference name="webviewbounce" value="false" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="deployment-target" value="7.0" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="default" />
    <preference name="AllowInlineMediaPlayback" value="true" />
    <preference name="android-windowSoftInputMode" value="stateVisible|adjustResize" />
    <content src="index.html" />
    <access launch-external="true" origin="tel:*" />
    <access launch-external="true" origin="geo:*" />
    <access launch-external="true" origin="mailto:*" />
    <access launch-external="true" origin="sms:*" />
    <access launch-external="true" origin="market:*" />
    <access origin="file:*" />
    <access origin="cdv:*" />
    <access origin="gap:*" />
    <access origin="http://meteor.local/*" />
    <access origin="*://192.168.0.68/*" />
    <access origin="*" />
    <platform name="ios">
        <icon height="60" src="resources/iphone.icon.png" width="60" />
        <icon height="29" src="resources/iphone.icon.png" width="29" />
        <icon height="40" src="resources/iphone.icon.png" width="40" />
        <icon height="50" src="resources/iphone.icon.png" width="50" />
        <icon height="57" src="resources/iphone.icon.png" width="57" />
        <icon height="58" src="resources/iphone.icon.png" width="58" />
        <icon height="120" src="resources/iphone_2x.icon.png" width="120" />
        <icon height="80" src="resources/iphone_2x.icon.png" width="80" />
        <icon height="100" src="resources/iphone_2x.icon.png" width="100" />
        <icon height="114" src="resources/iphone_2x.icon.png" width="114" />
        <icon height="180" src="resources/iphone_3x.icon.png" width="180" />
        <icon height="76" src="resources/ipad.icon.png" width="76" />
        <icon height="72" src="resources/ipad.icon.png" width="72" />
        <icon height="152" src="resources/ipad_2x.icon.png" width="152" />
        <icon height="144" src="resources/ipad_2x.icon.png" width="144" />
        <splash height="480" src="resources/iphone.splash.png" width="320" />
        <splash height="960" src="resources/iphone_2x.splash.png" width="640" />
        <splash height="1136" src="resources/iphone5.splash.png" width="640" />
        <splash height="1334" src="resources/iphone6.splash.png" width="750" />
        <splash height="2208" src="resources/iphone6p_portrait.splash.png" width="1242" />
        <splash height="1242" src="resources/iphone6p_landscape.splash.png" width="2208" />
        <splash height="1004" src="resources/ipad_portrait.splash.png" width="768" />
        <splash height="1024" src="resources/ipad_portrait.splash.png" width="768" />
        <splash height="2008" src="resources/ipad_portrait_2x.splash.png" width="1536" />
        <splash height="2048" src="resources/ipad_portrait_2x.splash.png" width="1536" />
        <splash height="748" src="resources/ipad_landscape.splash.png" width="1024" />
        <splash height="768" src="resources/ipad_landscape.splash.png" width="1024" />
        <splash height="1496" src="resources/ipad_landscape_2x.splash.png" width="2048" />
        <splash height="1536" src="resources/ipad_landscape_2x.splash.png" width="2048" />
    </platform>
    <platform name="android">
        <icon height="36" src="resources/android_ldpi.icon.png" width="36" />
        <icon height="42" src="resources/android_mdpi.icon.png" width="42" />
        <icon height="72" src="resources/android_hdpi.icon.png" width="72" />
        <icon height="96" src="resources/android_xhdpi.icon.png" width="96" />
        <splash density="port-ldpi" height="426" src="resources/android_ldpi_portrait.splash.png" width="320" />
        <splash density="land-ldpi" height="320" src="resources/android_ldpi_landscape.splash.png" width="426" />
        <splash density="port-mdpi" height="470" src="resources/android_mdpi_portrait.splash.png" width="320" />
        <splash density="land-mdpi" height="320" src="resources/android_mdpi_landscape.splash.png" width="470" />
        <splash density="port-hdpi" height="640" src="resources/android_hdpi_portrait.splash.png" width="480" />
        <splash density="land-hdpi" height="480" src="resources/android_hdpi_landscape.splash.png" width="640" />
        <splash density="port-xhdpi" height="960" src="resources/android_xhdpi_portrait.splash.png" width="720" />
        <splash density="land-xhdpi" height="720" src="resources/android_xhdpi_landscape.splash.png" width="960" />

        <hook type="after_prepare" src="scripts/androidAfterPrepare.js"/>
        <hook type="before_build" src="scripts/androidBeforeBuild.js"/>
    </platform>
    <preference name="xwalkVersion" value="15+" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect" />
    <preference name="xwalkMode" value="embedded" />
    <preference name="xwalkMultipleApk" value="true" />
</widget>
